[core]
	excludesfile = ~/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
	editor = /usr/bin/vim
[user]
  name = Rasmus Spiegelberg
[alias]
  co = checkout
  st = status
  d = diff
  re = !git for-each-ref --count=10 --sort=-committerdate refs/heads --format='%(HEAD)%(color:bold green)%(refname:short)|%(color:blue)%(committerdate:relative)|%(color:magenta)%(authorname)%(color:reset)' --color=always|column -ts'|'}
